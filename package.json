{
  "name": "ng1-template",
  "version": "1.0.0",
  "description": "AngularJS, Typescript and Gulp template for large applications",
  "scripts": {
    "postinstall": "node_modules/.bin/bower install && node_modules/.bin/typings install && node_modules/.bin/gulp setup",
    "start": "node_modules/.bin/gulp serve",
    "deploy": "node_modules/.bin/gulp build-dist --env=prod && firebase deploy",
    "dev": "node_modules/.bin/gulp serve --env=dev",
    "prod": "node_modules/.bin/gulp serve-dist --env=prod",
    "build-prod": "node_modules/.bin/gulp build-dist --env=prod",
    "code": "node_modules/.bin/gulp serve --launch=0",
    "vet": "node_modules/.bin/gulp vet"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-template/ng1-template.git"
  },
  "author": "Angular Template",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/angular-template/ng1-template/issues"
  },
  "homepage": "https://github.com/angular-template/ng1-template#readme",
  "devDependencies": {
    "gulp": "^3.9.0",
    "ng1-template-gulp": "^1.1.1"
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3"
  }
}
