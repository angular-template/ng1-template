{
  "name": "ng1-template",
  "version": "1.0.0",
  "description": "AngularJS, Typescript and Gulp template for large applications",
  "scripts": {
    "postinstall": "bower install && typings install && gulp setup",
    "start": "gulp serve",
    "deploy": "gulp build-dist --env=prod && firebase deploy",
    "dev": "gulp serve --env=dev",
    "prod": "gulp serve-dist --env=prod",
    "code": "gulp serve --launch=0",
    "vet": "gulp vet",
    "p": "npm prune && npm i ../ng1-template-gulp/ng1-template-gulp-1.0.8-beta.1.tgz"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-template/ng1-template.git"
  },
  "author": "Angular Template",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/angular-template/ng1-template/issues"
  },
  "homepage": "https://github.com/angular-template/ng1-template#readme",
  "devDependencies": {
    "gulp": "^3.9.0",
    "ng1-template-gulp": "^1.0.8"
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3"
  }
}
